<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<table name="t_customer" catalog="honeymzx"
	class="com.honey.model.Customer">
	<primary-key>
		<generator class="increment"></generator>

	</primary-key>
</table>
<table name="t_honey" catalog="honeymzx"
		class="com.honey.model.Honey">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="t_order" catalog="honeymzx"
	class="com.honey.model.Order">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>